trim(c.title ||
 '''' '''' || c.first_name || '''' '''' || c.other_names || '''' '''' || c.sur_name) "Full Name         ", 
 b.item_code_name  "Pay Item                ",		 
SUM(a.bals_amount) "Amount  ", 		 
