UPDATE rpt.rpt_reports SET report_name = ''Pay Run Master Sheet Report'', report_desc = ''Pay Run Master Sheet Report'', rpt_sql_query = ''SELECT tbl1.* FROM 
(SELECT 
c.local_id_no id_num, 
